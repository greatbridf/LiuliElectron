    "bootstrap": "^4.3.1",
    "css-loader": "^2.1.1",
    "electron": "^4.0.1",
    "electron-builder": "^20.42.0",
    "jquery": "^3.4.1",
    "popper.js": "^1.15.0",
    "style-loader": "^0.23.1",
    "ts-loader": "^6.0.2",
    "typescript": "^3.5.1",
    "vue": "^2.6.10",
    "vue-loader": "^15.7.0",
    "vue-template-compiler": "^2.6.10",
    "webpack": "^4.32.2",
    "webpack-cli": "^3.3.2"
