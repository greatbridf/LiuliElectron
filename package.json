{
  "name": "Hacg.me",
  "version": "1.7.0",
  "main": "dist/main.bundle.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/greatbridf/LiuliElectron.git"
  },
  "files": [
    "dist/",
    "static/",
    "package.json"
  ],
  "build": {
    "appId": "com.greatbridf.LiuliElectron",
    "directories": {
      "output": "release"
    },
    "mac": {
      "target": [
        "dmg"
      ],
      "publish": [
        "github"
      ]
    },
    "win": {
      "target": [
        "zip"
      ],
      "publish": [
        "github"
      ]
    }
  },
  "scripts": {
    "test": "electron . --debug",
    "start": "electron .",
    "dist": "npx webpack --config webpack.prod.js && electron-builder -mw",
    "pack": "npx webpack --config webpack.dev.js",
    "watch": "npx webpack --config webpack.dev.js --watch"
  },
  "devDependencies": {
    "@babel/core": "^7.4.5",
    "@types/jquery": "^3.3.29",
    "@types/node-fetch": "^2.3.4",
    "babel-loader": "^8.0.6",
    "bootstrap": "^4.3.1",
    "css-loader": "^2.1.1",
    "electron": "^7.2.4",
    "electron-builder": "^20.42.0",
    "jquery": "^3.4.1",
    "popper.js": "^1.15.0",
    "style-loader": "^0.23.1",
    "ts-loader": "^6.0.2",
    "typescript": "^3.5.1",
    "vue": "^2.6.10",
    "vue-loader": "^15.7.0",
    "vue-template-compiler": "^2.6.10",
    "webpack": "^4.32.2",
    "webpack-cli": "^3.3.2",
    "webpack-merge": "^4.2.1"
  },
  "dependencies": {}
}
