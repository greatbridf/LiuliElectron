{
  "name": "Hacg.me",
  "version": "1.5.1",
  "main": "dist/main.bundle.js",
  "build": {
    "appId": "com.greatbridf.LiuliElectron",
    "directories": {
      "output": "release"
    },
    "mac": {
      "target": [
        "dmg"
      ],
      "publish": [
        "github"
      ]
    },
    "win": {
      "target": [
        "zip"
      ],
      "publish": [
        "github"
      ]
    }
  },
  "scripts": {
    "test": "npx webpack && electron . --debug",
    "start": "npx webpack && electron .",
    "dist": "npx webpack && electron-builder -mw"
  },
  "devDependencies": {
    "css-loader": "^2.1.1",
    "electron": "^4.0.1",
    "electron-builder": "^20.42.0",
    "style-loader": "^0.23.1",
    "webpack": "^4.32.2",
    "webpack-cli": "^3.3.2"
  },
  "dependencies": {
    "request": "^2.88.0"
  }
}
